{
  "name": "@ktaicoder/blockly",
  "version": "6.20210701.0-patch3",
  "description": "Blockly library for ktaicoder",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ktaicoder/blockly.git"
  },
  "bugs": {
    "url": "https://github.com/ktaicoder/blockly/issues"
  },
  "homepage": "https://aicodingblock.kt.co.kr",
  "main": "index.js",
  "author": {
    "name": "ktaicoder"
  },
  "license": "MIT",
  "scripts": {
    "prepare": "yarn --cwd blockly-src install",
    "predist": "node ./scripts/copy-patch-files.js && yarn --cwd blockly-src build",
    "dist": "node ./scripts/dist-package.js && yarn --cwd blockly-src package && rimraf dist && cp -rf blockly-src/dist/* dist",
    "publish": "npm publish dist --access=public"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4"
  },
  "keywords": [
    "kt",
    "aicodingblock",
    "blockcoding"
  ]
}
